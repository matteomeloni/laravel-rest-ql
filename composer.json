{
    "name": "matteomeloni/laravel-rest-ql",
    "description": "A simple way, to be able to build a query into a Laravel model, through a http request",
    "license": "MIT",
    "authors": [
        {
            "name": "Matteo Meloni",
            "email": "matteomelonig@gmail.com",
            "homepage": "https://github.com/matteomeloni"
        }
    ],
    "homepage": "https://github.com/matteomeloni/laravel-rest-ql",
    "keywords": ["Laravel", "Eloquent", "Query", "Query Builder", "HTTP Request"],
    "require": {
        "php": "^7.4",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Matteomeloni\\LaravelRestQl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matteomeloni\\LaravelRestQl\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matteomeloni\\LaravelRestQl\\LaravelRestQlServiceProvider"
            ]
        }
    }
}
